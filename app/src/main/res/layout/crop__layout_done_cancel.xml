<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_alignParentBottom="true"
    style="@style/Crop.DoneCancelBar">

    <FrameLayout
        android:id="@+id/btn_cancel"
        style="@style/Crop.ActionButton">
        <com.sftelehealth.doctor.app.view.custom.CustomFontTextView style="@style/Crop.ActionButtonText.Cancel" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/btn_done"
        style="@style/Crop.ActionButton">
        <com.sftelehealth.doctor.app.view.custom.CustomFontTextView style="@style/Crop.ActionButtonText.Done" />
    </FrameLayout>

</LinearLayout>
